{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "env": {
    "browser": true,
    "es2023": true,
    "node": true
  },
  "root": true,
  "globals": {
    "Module": true,
    "Log": true,
    "MM": true,
    "d3": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2023
  },
  "rules": {
    "no-unused-vars": "warn",
    "prettier/prettier": [
      "error",
      {
        "useTabs": false,
        "tabWidth": 2
      }
    ]
  }
}
